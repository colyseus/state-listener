{
  "name": "delta-listener",
  "version": "1.0.0-alpha.1",
  "description": "Deeply compare JavaScript objects and listen to changes.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha lib/test/**.js",
    "prepublish": "rimraf lib && tsc",
    "pretest": "tsc -p tsconfig-test.json",
    "zuul": "zuul --ui mocha-bdd --no-coverage --concurrency 4 -- lib/test/*.js",
    "zuul-local": "zuul --ui mocha-bdd --no-coverage --concurrency 1 --local 8080 --disable-tunnel -- lib/test/*.js"
  },
  "keywords": [
    "patch",
    "compare",
    "delta"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.4",
    "zuul": "^3.11.1"
  }
}
